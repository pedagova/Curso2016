package missionaryCannibal;

import aima.core.search.framework.GoalTest;

/**
 * @author Ravi Mohan
 * 
 */
public class MissionaryCannibalGoalTest implements GoalTest {
	
	MissionaryCannibalBoard goal = new MissionaryCannibalBoard(new int[] { 0, 0, 1});
	 
	public boolean isGoalState(Object state) {
		return ((MissionaryCannibalBoard) state).equals(goal);
	}
}
